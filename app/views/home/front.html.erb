<div class="container">
    <div class="row">
        <div class="col-md-7 front-image">
            <br>
                <h1>　STUDY-SUPPORT</h1>
        </div>
        <div class="col-md-4 col-xs-offset-1">
    <h3>累計勉強時間上位10名</h3>
        <% @user.each do |user| %>
            <table>
                <tr>
                    <td>
                        <strong ><%= user.experience_point %>時間</strong> 
                    </td>
                    <td>
                        <strong >Lv.<%= user.level %></strong> 
                    </td>
                    <td>
                        <%= user.last_name %>
                    </td>
                    <td>
                        <%= user.first_name %> 
                    </td>
                </tr>
            </table>
        <% end %>
    </div>
</div>



